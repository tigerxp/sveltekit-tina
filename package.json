{
	"name": "sveltekit-tina",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "tinacms dev -c \"vite dev\"",
		"build": "tinacms build && vite build",
		"preview": "tinacms build && vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@auth/sveltekit": "^1.10.0",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tinacms/cli": "^1.10.2",
		"@tinacms/datalayer": "^1.4.1",
		"@types/node": "^24.3.0",
		"@upstash/redis": "^1.35.3",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"mdsvex": "^0.12.3",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tinacms": "^2.8.2",
		"tinacms-authjs": "^11.0.2",
		"tinacms-gitprovider-github": "^3.0.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"upstash-redis-level": "^1.1.1",
		"vite": "^7.0.4"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
